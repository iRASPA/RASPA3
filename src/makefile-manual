CXX=clang++
DESTDIR=${HOME}/raspa3
#CXX=~/software/bin/clang++
VERSION=3.0.16
DBGCXXFLAGS = -g3 -O0 -DDEBUG -fno-omit-frame-pointer -fno-optimize-sibling-calls -fsanitize=address -fsanitize=leak -fsanitize=undefined
RELCXXFLAGS = -g -O3 -march=native -ffast-math
COMMONFLAGS= -DVERSION=${VERSION} -I/usr/local/opt/libomp/include -I/usr/local/include -fexperimental-library -std=c++2b -fmodules -fbuiltin-module-map -stdlib=libc++ -fopenmp -Wall -Wextra -Wshadow -Wnon-virtual-dtor -Wold-style-cast -Wcast-align -Wunused -Woverloaded-virtual -Wpedantic -Wconversion -Wsign-conversion -Wnull-dereference -Wdouble-promotion -Wformat=2 -Wno-gnu-anonymous-struct  -Wno-gnu-anonymous-struct -Wno-error=deprecated-declarations -Wno-deprecated-declarations -fomit-frame-pointer -ftree-vectorize -fno-stack-check -funroll-loops

MODULEPATHS = -fprebuilt-module-path=. -fprebuilt-module-path=./foundationkit -fprebuilt-module-path=./mathkit -fprebuilt-module-path=./symmetrykit -fprebuilt-module-path=./raspakit -fprebuilt-module-path=./raspakit/interactions -fprebuilt-module-path=./raspakit/mc_moves -fprebuilt-module-path=./raspakit/potentials -fprebuilt-module-path=./raspakit/properties -fprebuilt-module-path=./raspakit/cbmc -fprebuilt-module-path=./raspakit/forcefield

PYTHONFLAGS = $(shell python3 -m pybind11 --includes)
# option --embed needed to link with the python library
#PYTHONLDFLAGS = $(shell python3-config --ldflags --embed)
PYTHONLDFLAGS = $(shell python3-config --ldflags)

IXX    := $(wildcard *.ixx foundationkit/*.ixx mathkit/*.ixx symmetrykit/*.ixx raspakit/*.ixx raspakit/interactions/*.ixx \
					  raspakit/mc_moves/*.ixx raspakit/potentials/*.ixx raspakit/properties/*.ixx raspakit/cbmc/*.ixx raspakit/forcefield/*.ixx)
PCM    := $(IXX:.ixx=.pcm)
OBJPCM := $(IXX:.ixx=.pcm.o)
CPP    := $(wildcard *.cpp foundationkit/*.cpp mathkit/*.cpp symmetrykit/*.cpp raspakit/*.cpp raspakit/interactions/*.cpp \
					  raspakit/mc_moves/*.cpp raspakit/potentials/*.cpp raspakit/properties/*.cpp raspakit/cbmc/*.cpp raspakit/forcefield/*.cpp)
OBJ    := $(CPP:.cpp=.o)


.PHONY: release
release: CXXFLAGS = -g -O3 -march=native -ffast-math $(COMMONFLAGS)
release: raspa3

.PHONY: debug 
debug: CXXFLAGS = -g3 -O0 -DDEBUG -fno-omit-frame-pointer -fno-optimize-sibling-calls -fsanitize=address -fsanitize=leak -fsanitize=undefined $(COMMONFLAGS)
debug: raspa3

.PHONY: shared
shared: CXXFLAGS = -fpic -g -O3 -march=native -ffast-math $(COMMONFLAGS)
shared: raspa3lib


.SUFFIXES:
.SUFFIXES: .pcm .ixx *.cpp

.SECONDARY:

$(OBJ): %.o: %.cpp %.pcm
	$(CXX) $(CXXFLAGS) $(MODULEPATHS) $(PYTHONFLAGS) $< -c -o $@

%.pcm: %.ixx
	$(CXX) $(CXXFLAGS) $(MODULEPATHS) $(PYTHONFLAGS) -x c++-module --precompile -o $@ $<
	$(CXX) $(CXXFLAGS) $(MODULEPATHS) $(PYTHONFLAGS) -x c++-module $< -c -o $@.o

