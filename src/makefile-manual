raspa3: $(OBJ)
	$(CXX) $(CXXFLAGS) $(MODULEPATHS) ../app/main.cpp -c -o ../app/main.o
	$(CXX) $(CXXFLAGS) -L/usr/local/opt/libomp/lib -flto -stdlib=libc++ -o ../app/raspa ../app/main.o foundationkit/*.o mathkit/*.o symmetrykit/*.o raspakit/*.o raspakit/interactions/*.o raspakit/mc_moves/*.o raspakit/potentials/*.o raspakit/properties/*.o raspakit/cbmc/*.o raspakit/forcefield/*.o -llapack -lblas -fopenmp $(PYTHONLDFLAGS)

raspa3lib: $(OBJ)
	$(CXX) -shared mathkit/*.o foundationkit/*.o symmetrykit/*.o raspakit/*.o raspakit/interactions/*.o raspakit/mc_moves/*.o raspakit/potentials/*.o raspakit/properties/*.o raspakit/cbmc/*.o raspakit/forcefield/*.o $(PYTHONLDFLAGS) -llapack -lblas -fopenmp  -o ../python/raspakit.so

.PHONY: install
install:
	test -d $(DESTDIR) || mkdir $(DESTDIR)
	test -d $(DESTDIR)/bin || mkdir $(DESTDIR)/bin
	install -m 755 ../app/raspa  $(DESTDIR)/bin
	test -d $(DESTDIR)/share || mkdir $(DESTDIR)/share
	test -d $(DESTDIR)/share/raspa3 || mkdir $(DESTDIR)/share/raspa3
	test -d $(DESTDIR)/share/raspa3/forcefields || mkdir $(DESTDIR)/share/raspa3/forcefields
	test -d $(DESTDIR)/share/raspa3/forcefields/example_molecule_forcefield || mkdir $(DESTDIR)/share/raspa3/forcefields/example_molecule_forcefield
	install -m 664 ../data/forcefields/example_molecule_forcefield/force_field.json  $(DESTDIR)/share/raspa3/forcefields/example_molecule_forcefield/force_field.json
	test -d $(DESTDIR)/share/raspa3/molecules || mkdir $(DESTDIR)/share/raspa3/molecules
	test -d $(DESTDIR)/share/raspa3/molecules/example_definitions || mkdir $(DESTDIR)/share/raspa3/molecules/example_definitions
	install -m 664 ../data/molecules/example_definitions/methane.json  $(DESTDIR)/share/raspa3/molecules/example_definitions/methane.json
	install -m 664 ../data/molecules/example_definitions/co2.json  $(DESTDIR)/share/raspa3/molecules/example_definitions/co2.json
	install -m 664 ../data/molecules/example_definitions/n2.json  $(DESTDIR)/share/raspa3/molecules/example_definitions/n2.json

clean:
	rm -f ../app/raspa *.pcm *.o foundationkit/*.pcm foundationkit/*.o mathkit/*.pcm mathkit/*.o symmetrykit/*.pcm symmetrykit/*.o raspakit/*.pcm raspakit/*.o raspakit/interactions/*.pcm raspakit/interactions/*.o raspakit/mc_moves/*.o raspakit/potentials/*.o raspakit/properties/*.o raspakit/cbmc/*.o raspakit/forcefield/*.o ../app/main.pcm ../app/main.o *.so ../app/raspa3.exe
